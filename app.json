{
  "expo": {
    "name": "ENERGYRIA",
    "slug": "energyria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ENERGYRIA.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ENERGYRIA.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.releveurcompteur.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ENERGYRIA.png",
        "backgroundColor": "#6366f1"
      },
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.INTERNET"
      ],
      "package": "com.releveurcompteur.app"
    },
    "web": {
      "favicon": "./assets/ENERGYRIA.png"
    },
    "extra": {
      "eas": {
        "projectId": "2a3e86ff-1048-4bbd-aa93-bcb25a12cac9"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
